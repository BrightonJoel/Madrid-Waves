    "Likes": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "Views": {
      "type": "integer",
      "default": 0
    },
    "isLiked": {
      "type": "boolean",
      "default": false
    },
    "UserId": {
      "type": "integer"
    },
    "users_permissions_user": {
      "columnName": "Users",
      "plugin": "users-permissions",
      "model": "user",
      "via": "blogs"
